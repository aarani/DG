<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    x:Class="Frontend.AboutPage"
    Title="About Page"
>
    <StackLayout Margin="10">
        <Label>
            <Label.FormattedText>
                <FormattedString>
                    <Span Text="Version number: " />
                    <Span x:Name="VersionNumberLabel" />
                </FormattedString>
            </Label.FormattedText>
        </Label>
    </StackLayout>
</ContentPage>
